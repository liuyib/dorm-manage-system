<Page x:Class="大作业.Pages.PageCalendar"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:大作业.Pages"
      xmlns:z="wpfz"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="600"
      Title="PageCalendar">
    <StackPanel Orientation="Horizontal">
        <Calendar Margin="3" Style="{StaticResource CalendarDefaultStyle}"/>
        <Calendar Margin="3" Style="{StaticResource CalendarDefaultStyle}" IsEnabled="False"/>
    </StackPanel>
</Page>
